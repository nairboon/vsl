!!!
html(ng-app="myApp")
  head
    meta(charset='utf8')
    base(href='/')
    title Angular Socket.io Seed App

    link(rel='stylesheet', href='/bower_components/bootstrap/dist/css/bootstrap.css')
    link(rel='stylesheet', href='/bower_components/ng-grid/ng-grid.css')
    link(rel='stylesheet', href='/bower_components/jquery-ui/themes/smoothness/jquery-ui.css')
    link(rel='stylesheet', href='/bower_components/angular-ui-slider/src/ui-slider.css')
    link(rel='stylesheet', href='/css/app.css')
  body
   header
    nav.navbar.navbar-default(ng-controller='NavCtrl')
      .navbar-header
        a.navbar-brand(href='/index') ABST
      
      .navbar-collapse.collapse
            ul.nav.navbar-nav
              li
                a(href='/projects') Projects
              li
                a(href='/projects/{{project}}/models') Models
     #nav-bars-container(ng-controller='NavCtrl')
      a(href='#/{{project.id}}') Foo

    block body

  script(src='/socket.io/socket.io.js')
  script(src='bower_components/lodash/dist/lodash.js')
  script(src='bower_components/angular/angular.js')
  script(src='bower_components/angular-route/angular-route.js')
  script(src='bower_components/angular-socket-io/socket.js')
  script(src='bower_components/jquery/jquery.js')
  script(src='bower_components/jquery-ui/ui/jquery-ui.js')
  script(src='bower_components/bootstrap/dist/js/bootstrap.js')
  script(src='bower_components/angular-bootstrap/ui-bootstrap-tpls.js')
  script(src='bower_components/restangular/dist/restangular.js')
  script(src='bower_components/ng-grid/build/ng-grid.js')
  script(src='bower_components/angular-ui-slider/src/ui-slider.js')

  script(src='js/app.js')
  script(src='js/services.js')
  script(src='js/controllers.js')
  script(src='js/filters.js')
  script(src='js/directives.js')
